{
  "name": "teachers-api",
  "version": "1.0.0",
  "description": "Simple express-mysql server",
  "main": "index.js",
  "scripts": {
    "start": "node -r @babel/register -r ./index.js",
    "dev": "nodemon index",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "precover": "rm -rf coverage",
    "cover": "jest --coverage --collectCoverageFrom=src/controllers/*.js",
    "postcover": "cp coverage/coverage-final.json coverage/coverage.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run cover"
    }
  },
  "keywords": [
    "express",
    "server",
    "backend"
  ],
  "author": "Siddhardha Tagirisa",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.7",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-core": "^6.26.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-watch": "^7.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
